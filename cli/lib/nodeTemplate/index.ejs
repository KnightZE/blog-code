<% } -%>
<% if (modules.indexOf("fs")> -1) { -%>
const fs = require("fs");
<% } -%>
<% if (modules.indexOf("axios")> -1) { -%>
const axios = require("axios");
<% } -%>
<% if (modules.indexOf("qs")> -1) { -%>
